    "@types/express": "^5.0.6",
    "@types/node": "^25.0.10",
    "@voidzero-dev/vitepress-theme": "^4.4.1",
    "chalk": "^5.6.2",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.4.0",
    "feed": "^5.2.0",
    "gray-matter": "^4.0.3",
    "gsap": "^3.14.2",
    "lint-staged": "^16.2.7",
    "lodash": "^4.17.23",
    "markdown-it-image-size": "^15.0.1",
    "simple-git-hooks": "^2.13.1",
    "vite": "^7.3.1",
    "vitepress": "2.0.0-alpha.15",
    "vitepress-plugin-group-icons": "^1.7.1",
    "vue": "^3.5.27",
    "vue-tsc": "^3.2.4",
    "yorkie": "^2.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
