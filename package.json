{
  "name": "vite-docs-cn",
  "type": "module",
  "version": "8.0.0-beta.10",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "description": "Vite.js documentation Chinese translation.",
  "author": "Evan You",
  "license": "CC BY-NC-SA 4.0",
  "repository": "https://github.com/vitejs/docs-cn",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
